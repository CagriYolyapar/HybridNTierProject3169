@model UserRegisterModel

<center>
    <h1>
        SignIn'e hosgeldiniz
        @TempData["Message"]

    </h1>


    <form asp-action="SignIn" asp-controller="Home" method="post" class="p-3 w-75 m-auto">
        <div class="mb-3">
            <label>Kullanıcı ismi giriniz</label>
            <input asp-for="UserName" class="form-control" />
        </div>

        <div class="mb-3">
            <label>Sifre giriniz</label>
            <input asp-for="Password" class="form-control" />
        </div>


        <div class="mb-3">
            <button class="btn btn-primary">Giris yap</button>
        </div>
    </form>

</center>
